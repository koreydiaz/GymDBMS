<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.database.gymdbms_application.EmployeeController">
    <children>
        <Button layoutX="902.0" layoutY="28.0" mnemonicParsing="false" onAction="#switchToMembers" prefHeight="28.0" style="-fx-background-color: #4672c4;" text="Members" textFill="WHITE" />
        <Button layoutX="835.0" layoutY="28.0" mnemonicParsing="false" onAction="#switchToHome" prefHeight="28.0" style="-fx-background-color: #4672c4;" text="Home" textFill="WHITE" />
        <Button layoutX="551.0" layoutY="561.0" mnemonicParsing="false" onAction="#addEmployee" text="+" />
        <Button layoutX="515.0" layoutY="561.0" mnemonicParsing="false" onAction="#deleteEmployee" prefHeight="25.0" prefWidth="25.0" text="-" />
        <TextField alignment="CENTER" layoutX="396.0" layoutY="78.0" onAction="#searchForEmployee" prefHeight="25.0" prefWidth="180.0" text="Search by name" AnchorPane.leftAnchor="396.0" AnchorPane.rightAnchor="424.0" />
        <Label layoutX="14.0" layoutY="13.0" text="Employees Page">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <TableView fx:id="employeeTable" layoutX="14.0" layoutY="109.0" prefHeight="447.0" prefWidth="562.0">
            <columns>
                <TableColumn fx:id="empID" prefWidth="90.0" text="Employee ID" />
                <TableColumn fx:id="empFname" prefWidth="113.0" text="First Name" />
            <TableColumn fx:id="empLname" prefWidth="102.0" text="Last Name" />
            <TableColumn fx:id="empBday" prefWidth="88.0" text="Birth Date" />
            <TableColumn fx:id="empPlocation" prefWidth="107.0" text="Primary Location" />
            <TableColumn fx:id="empType" prefWidth="61.0" text="Type " />
            </columns>
        </TableView>
    </children>
</AnchorPane>
